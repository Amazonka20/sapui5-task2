                <IconTabFilter id="tabJson" key="json" text="JSON Model" />
                <IconTabFilter id="tabODataV2" key="odataV2" text="OData V2 Model" />
                <IconTabFilter id="tabODataV4" key="odataV4" text="OData V4 Model" />
            </items>
        </IconTabBar>
    </Page>
</mvc:View>
